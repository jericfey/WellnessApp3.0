  "name": "wellnessapp",
  "version": "1.0.0",
  "description": "Follaine Wellness App",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently -n 'server, client' -c 'red,green' \"npm run server\" \"npm run client\"",
